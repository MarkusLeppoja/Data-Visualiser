<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Data</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Data_Visualiser.styles.css" asp-append-version="true" />
</head>


<body>
    <nav class="navbar navbar-light fixed-bottom p-2 shadow" style="background-color: #198754;">
        <a class="navbar-brand fs-5" style="text-shadow: lg; color: lightgray"; asp-area="" asp-page="/Index">Home</a>
        <a class="navbar-brand fs-5" style="text-shadow: lg; color: lightgray"; asp-area="" asp-page="/Motor/MotorDataView">Sugar Motor</a>
    </nav>


    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>